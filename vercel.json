{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.5.3"
        }
    },
    "routes": [
        { "src": "/build/(.*)", "dest": "/public/build/" },
        { "src": "/public/(.*)", "dest": "/public/" },
        { "src": "/resources/(.*)", "dest": "/public/resources/" },
        { "src": "/favicon/(.*)", "dest": "/public/favicon/" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],

    "outputDirectory": "public/build",
    "env": {
        "APP_NAME": "Vercel Laravel",
        "APP_ENV": "production",
        "APP_KEY": "base64:t3pRRIHrGPfFvnorjaGM9wViFci4LEMfVpShlr0wtQE=",
        "APP_DEBUG": "false",
        "APP_URL": "https://laravel-vercel-movie.vercel.app",
        "VERCEL_DEMO_MODE": "true",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "VIEW_COMPILED_PATH": "/tmp/views",
        "SSR_TEMP_PATH": "/tmp/ssr",
        "NODE_PATH": "node"
    }
}
